{
  "name": "slice-dice",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "serve": "tsx index.ts",
    "fmt": "prettier --write .",
    "lint": "oxlint",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:drop": "drizzle-kit drop",
    "db:pull": "drizzle-kit introspect:sqlite",
    "db:push": "drizzle-kit push:sqlite",
    "db:check": "drizzle-kit check:sqlite",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx seed.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.4.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.4",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "jose": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "drizzle-kit": "^0.20.14",
    "oxlint": "^0.2.8",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0"
  },
  "prettier": {}
}
